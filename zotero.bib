@article{wierenga_prediction_1986,
	title = {Prediction of the occurrence of the {ADP}-binding $\beta$$\alpha$$\beta$-fold in proteins, using an amino acid sequence fingerprint},
	volume = {187},
	issn = {0022-2836},
	doi = {10.1016/0022-2836(86)90409-2},
