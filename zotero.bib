	pmid = {11173978},
	keywords = {Alcohol Dehydrogenase, Animals, Catalytic Domain, Cloning, Molecular, Ethanol, formaldehyde, Glutathione, Humans, Isoenzymes, Kinetics, Mammals, Oxidation-Reduction, Rats, Recombinant Proteins, Serotonin, Vitamin A},
