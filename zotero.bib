	pages = {830--844},
	file = {ScienceDirect Full Text PDF:/home/talavis/.zotero/zotero/etop8sp2.default/zotero/storage/CWVSUFM7/Chen et al. - 2008 - Structural Enzymological Studies of 2-Enoyl Thioes.pdf:application/pdf;ScienceDirect Snapshot:/home/talavis/.zotero/zotero/etop8sp2.default/zotero/storage/4VUFQDJR/S0022283608004889.html:text/html;Structural enzymological studies of 2-enoyl thioester reductase of the human mitochondrial FAS... - Abstract - Europe PubMed Central:/home/talavis/.zotero/zotero/etop8sp2.default/zotero/storage/9CMMF5D7/reload=0\;jsessionid=s5Z0gbcXo3Gz76pv1GAu.html:text/html}
